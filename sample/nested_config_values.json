{
    "model": {
        "type": "boosted_tree",
        "parameters": {
            "max_depth": 5,
            "max_leaves": 31,
            "bagging_fraction": 0.5,
            "eta": 0.03,
            "learning_rate": 0.01
        }
    },
    "training": {
        "data": {
            "paths": [
                "data/00*.csv",
                "data/01*.csv"
            ],
            "augmentation": [
                {
                    "method": "fillna",
                    "column": "x",
                    "kwargs": {
                        "value": -1
                    }
                }
            ]
        }
    },
    "evaluation": {
        "data": {
            "paths": [
                "data/05*.csv"
            ]
        }
    }
}